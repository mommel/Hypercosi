#!/bin/sh
DIRNAME=$(dirname "$0")
if [ "${DIRNAME}" == ".yarnscripts" ]; then
  echo "" > ../audit/audit-output.json
  yarn audit --json > ../audit/audit-output.json
  yarn audit
elif [ "${DIRNAME}" == "Hypercosi" ]; then
   echo "" > audit/audit-output.json
  yarn audit --json > audit/audit-output.json
  yarn audit
else
   echo "Unknown Folder"
fi
