{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2017": true,
    "jest": true,
    "node": true
  },
  "extends": ["prettier"],
  "plugins": ["prettier", "react", "jsx-a11y"],
  "parser": "Babel-ESLint",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "configs/webpack.config.eslint.js"
      }
    },
    "react": {
      "pragma": "React",
      "version": "16.8.6"
    }
  },
  "rules": {}
}
